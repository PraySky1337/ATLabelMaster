{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug controller_node",
            "type": "cppdbg",
            "request": "launch",
            "program": "/bin/bash",
            "args": [
                "-c",
                "source /opt/ros/humble/setup.bash && source ${workspaceFolder}/install/setup.bash && exec ${workspaceFolder}/install/solver/lib/solver/controller_node"
            ],
            "cwd": "${workspaceFolder}",
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
